<% title "Editing #{h @feature.name}" %>
<h1><%= yield :title %></h1>

<%= error_messages_for :feature %>

<% form_for(:feature, :url => feature_path(@feature), :html => { :method => :put }) do |f| %>
<%= render :partial => "form", :locals => { :f => f } %>
<p><%= submit_tag "Update" %></p>
<% end %>